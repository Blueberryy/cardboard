{
  "application": {
    "id": 436989367941070848,
    "manifests": [
      {
        "label": "crimson/windows",
        "platforms": ["win32", "win64"],
        "locales": [],
        "local_root": ".",
        "file_rules": {
          "mappings": [
            {
              "local_path": ".",
              "install_path": "."
            }
          ],
          "exclusions": [
            {
              "local_path": "**/*.pdb"
            }
          ]
        },
        "launch_options": [
          {
            "name": "Project Crimson x64",
            "executable": "bin64/cardboard_msvc.exe",
            "arguments": ["\"-q$HOME/My Games/Project Crimson Alpha\" -glog.txt"],
            "platforms": ["win64"],
            "working_dir": "."
          },
          {
            "name": "Project Crimson x86",
            "executable": "bin/cardboard_msvc.exe",
            "arguments": ["\"-q$HOME/My Games/Project Crimson Alpha\" -glog.txt"],
            "platforms": ["win32"],
            "working_dir": "."
          }
        ],
          "storage": {
          "sync": true,
          "roots": [
            {
              "id": "config-and-maps",
              "paths": [
                {
                  "platform": "windows",
                  "path": "${DOCUMENTS}/My Games/Project Crimson Alpha"
                }
              ],
              "patterns": ["**/*"]
            }
          ]
        }
      }
    ]
  }
}